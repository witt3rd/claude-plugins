{
  "name": "azkg",
  "version": "0.1.0",
  "displayName": "Agentic-ZKG",
  "description": "Agent-maintained Zettelkasten knowledge graph",
  "author": "Donald Thompson",
  "license": "MIT",
  "repository": "https://github.com/witt3rd/claude-plugins",
  "commands": [
    "create-note",
    "search-notes",
    "expand-graph",
